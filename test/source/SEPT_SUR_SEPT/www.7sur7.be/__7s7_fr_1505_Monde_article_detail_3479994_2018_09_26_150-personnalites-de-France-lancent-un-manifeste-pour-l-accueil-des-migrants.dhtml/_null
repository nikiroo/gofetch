<h1 class="k1 mrg" id="articleDetailTitle">150 personnalités de France lancent un &quot;manifeste pour l'accueil des migrants&quot;</h1>
<section class="str_article_cntr buttons-positioning">
<script type="text/javascript">
function getUrlVars() {
var vars = {};
var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
vars[key] = value;
});
return vars;
}
</script>
