{
$('#teas_emopoll').addClass("voted");
var vote = $(this).attr('id');
var emo  = vote.substr(vote.length-1);
emoVotes[3480024] = emo;
$.jStorage.set("emoVotes",emoVotes);
$('.vote').unbind();
$('#vote-emo' + emo).addClass("emoactive");
$.post('/7s7/article/emoVotes.do',
